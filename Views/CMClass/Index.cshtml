@model IEnumerable<KaizenAPI.Models.CMClass>

@{
    ViewBag.Title = "CM Class";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Class_Id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Class)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Sections)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Class_Id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Class)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Sections)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { @id = item.Id }) |
            @Html.ActionLink("Details", "Details", new { @id = item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { @id = item.Id })
        </td>
    </tr>
}
</table>
